@model MVCKutuphane.Models.tblpersoneller
@{
    ViewBag.Title = "PersonelGetir";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using (Html.BeginForm("PersonelGuncelle", "Personel", FormMethod.Post))
{

  <div>
      <br />
      <label>Personel ID</label><br />
      @Html.TextBoxFor(m => m.ID, new { @class = "form-control" })
<br />
      <label>Personel Adı</label><br />
      @Html.TextBoxFor(m => m.PERSONEL, new { @class = "form-control" })

  </div><br />
  <button class="btn btn-warning">Personel Güncelle</button>
}
